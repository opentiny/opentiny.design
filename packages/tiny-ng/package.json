{
  "name": "tiny-ng",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start:innerdocs": "node ./scripts/copy.js innerdocs && vite --config vite.config.innerdocs.js",
    "start:opendocs": "node ./scripts/copy.js opendocs && vite --config vite.config.opendocs.js",
    "start:tinycloud": "node ./scripts/copy.js tinycloud && vite --mode tinycloud --config vite.config.tinycloud.js",
    "build:innerdocs": "node ./scripts/copy.js innerdocs && vite build --config vite.config.innerdocs.js",
    "build:opendocs": "node ./scripts/copy.js opendocs && vite build --config vite.config.opendocs.js",
    "build:tinycloud": "node ./scripts/copy.js tinycloud && vite build --mode tinycloud --config vite.config.tinycloud.js",
    "eslint": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "npx prettier --write ./**/*.{ts,tsx,css,less,scss}",
    "stylelint": "npx stylelint ./src/**/*.scss ./src/**/*.less ./src/**/*.css --fix",
    "lint": "npm run eslint && npm run prettier && npm run stylelint"
  },
  "dependencies": {
    "@opentiny/vue": "3.12.1",
    "@unhead/vue": "1.8.9",
    "@unocss/reset": "0.38.2",
    "dompurify": "^3.0.1",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "intersection-observer": "^0.12.2",
    "lz-string": "^1.5.0",
    "marked": "^4.3.0",
    "prismjs": "^1.28.0",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.45",
    "@unocss/preset-icons": "^0.38.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.37",
    "babel-eslint": "^10.1.0",
    "chalk": "4.1.2",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.3.0",
    "fs-extra": "^10.1.0",
    "less": "^4.1.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-prism": "^2.2.4",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "minimist": "^1.2.6",
    "naive-ui": "^2.30.6",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "unocss": "^0.39.3",
    "unplugin-auto-import": "0.8.7",
    "unplugin-vue-components": "^0.19.9",
    "unplugin-vue-markdown": "^0.25.2",
    "uslug": "^1.0.4",
    "vite": "^2.9.12",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-inspect": "^0.5.0",
    "vue-eslint-parser": "^7.3.0"
  }
}
